#!/usr/bin/env python3

import argparse
from src.cooperace import Cooperace

parser = argparse.ArgumentParser(description="Run Cooperace with specified parameters")

parser.add_argument('--arch', required=False)
parser.add_argument('--prop', required=False)
parser.add_argument('filepath')

args = parser.parse_args()

cooperace = Cooperace(args.filepath, args.prop, args.arch)
    
print(cooperace.start())
